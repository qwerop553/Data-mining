reg <- function(y, x){
x <- as.matrix(x)
x <- cbind(Intercept = 1, x)
b <- solve(t(x) %*% x) %*% t(x) %*% y
colnames(b) <- "estimate"
print(b)
}
challenger <- read.csv("challenger.csv")
reg(challenger$distress_ct, challenger$temperature)
